{
  "name": "@brotsky/tools",
  "version": "0.1.0",
  "description": "Reusable code library for React components, Prisma schemas, GraphQL server code and more",
  "module": "dist/index.js",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "rm -rf dist && tsc && bun build ./src/index.ts --outdir ./dist --target node",
    "ts-errors": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"__tests__/**/*.{ts,tsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "bun test",
    "prepublishOnly": "bun run build && bun format",
    "codegen": "graphql-codegen --config codegen.ts && prettier --write ./src/gql/client/generated.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "ladle": "ladle serve",
    "build-ladle": "ladle build",
    "vercel-deploy": "bun run build-ladle && bun format"
  },
  "keywords": ["react", "prisma", "graphql", "components", "utilities", "nextjs"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@emotion/css": "^11.13.5",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.5.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@ladle/react": "^5.0.1",
    "@prisma/client": "^6.4.1",
    "@storybook/addon-a11y": "^8.6.4",
    "@storybook/addon-docs": "^8.6.4",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-interactions": "^8.6.4",
    "@storybook/addon-links": "^8.6.4",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/builder-webpack5": "^8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/react-webpack5": "^8.6.4",
    "@storybook/types": "^8.6.4",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^4.0.12",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
